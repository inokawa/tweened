{
  "name": "tweened",
  "version": "0.1.0",
  "description": "A simple, declarative and composable animation library for React.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "tsc": "tsc -p . --noEmit",
    "tsc:d": "tsc -p . --outDir lib -d --emitDeclarationOnly",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "dependencies": {
    "d3-color": "^3.0.0",
    "d3-ease": "^3.0.0",
    "d3-interpolate": "^3.0.0",
    "d3-timer": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@rollup/plugin-typescript": "8.3.0",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/d3": "7.1.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.2.3",
    "d3": "^7.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.67.2",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": ">= 16.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/tweened.git"
  },
  "keywords": [
    "react",
    "hooks",
    "animation",
    "tween",
    "transition",
    "visualization",
    "svg",
    "css",
    "d3",
    "web-animations"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/tweened/issues"
  },
  "homepage": "https://github.com/inokawa/tweened#readme"
}
